    "htm=> create table dropme as (WITH A AS (\n",
    "SELECT\n",
    "  queue,\n",
    "  runtime/3600.0 wct,\n",
    "  ((utime+stime+.0)/numprocessors)/3600 cpt_h,\n",
    "  ((utime+stime+.0)/numprocessors)/runtime jeff,\n",
    "  maxrmem/numprocessors/1e6 ram\n",
    "FROM htjob WHERE\n",
    "  runtime > 300 AND utime > 0 AND\n",
    "  eventtimeepoch BETWEEN to_unixtime('2021-09-01') AND\n",
    "                         to_unixtime('2021-09-02')\n",
    "), B AS (\n",
    "SELECT\n",
    "  count(*) n,\n",
    "  queue\n",
    "FROM A \n",
    "GROUP BY queue\n",
    "),\n",
    "C AS (\n",
    "SELECT row_number() OVER (ORDER BY n DESC) clust,queue FROM B)\n",
    "SELECT C.clust, A.* FROM A,C WHERE\n",
    "  A.queue = C.queue);\n",
    "\n",
    "htm=> select clust, count(*) n, queue from dropme group by clust,queue order by n desc;\n",
    " clust |   n   |  queue   \n",
    "-------+-------+----------\n",
